var pgn = "<%= j @game.pgn %>";
var chess = new Chess();
console.log(chess.fen());
var ok = chess.load_pgn(pgn, { sloppy: true });
console.log('ok is: ' + ok);
var board = Chessboard('board', {
    showNotation: false,
    position: ok ? chess.fen() : 'start',
  });
$(window).resize(function() {
  board.resize();
});
